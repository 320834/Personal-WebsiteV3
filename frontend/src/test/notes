let distance = Math.sqrt(Math.pow(x-(window.innerWidth/2),2) + Math.pow(y-(window.innerHeight/2),2));
            
console.log(distance)
let f = 0;
if(distance < 400)
{
    f = distance;
}
else
{
    f = -distance;
}

